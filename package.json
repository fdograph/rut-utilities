{
  "name": "@fdograph/rut-utilities",
  "version": "1.3.8",
  "repository": "https://github.com/fdograph/rut-utilities",
  "description": "Fully typed set of utility functions to parse, validate and generate a Chilean R.U.T. \n Set de funciones utilitarias para generar, procesar y validar un R.U.T. Chileno completamente tipado.",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "build:prod": "npm run clean && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage && jest-coverage-badges --output './badges'",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "chile",
    "chilean",
    "chileno",
    "validate",
    "validar",
    "validator",
    "validador",
    "run",
    "rut",
    "r.u.t",
    "r.u.n",
    "rut chileno",
    "cedula",
    "identidad",
    "comprobar",
    "comprobador",
    "empresas",
    "dv"
  ],
  "author": "Fernando Silva M. (@fdograph)",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
