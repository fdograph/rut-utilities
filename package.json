{
  "name": "@fdograph/rut-utilities",
  "version": "1.2.2",
  "repository": "https://github.com/fdograph/rut-utilities",
  "description": "Utility functions to parse, validate and generate a Chilean R.U.T.",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage && jest-coverage-badges --output './badges'",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "chile",
